@model LUser
<form action="LogCheck" method="post">
    <div class="form-group">
        <label>Email</label>
        <span asp-validation-for="LEmail" style="color:red;"></span>
        <input class="form-control" asp-for="LEmail" value="h@gmail.com">
    </div>
    <div class="form-group">
        <label>Password</label>
        <span asp-validation-for="LPassword" style="color:red;"></span>
        <input class="form-control" asp-for="LPassword" value="asdf">
    </div>
    <input type="submit" class="btn btn-primary">
</form>